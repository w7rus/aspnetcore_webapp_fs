{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Hangfire": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "MiscOptions": {
    "ContentPath": "content",
    "CorsAllowedOrigins": [
      "*"
    ],
    "IsFilePreviewsEnabled": false,
    "FilePreviewMaxLongEdgeLength": 512,
    "AuthenticationAccessToken" : ""
  },
  "SeqOptions": {
    "Endpoint": {
      "Scheme": "",
      "Host": "",
      "Port": 5341
    },
    "ApiKey": ""
  }
}
