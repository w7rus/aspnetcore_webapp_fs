{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Hangfire": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "MiscOptions": {
    "ContentPath": "content",
    "CorsAllowedOrigins": [
      "https://localhost:5002",
      "http://localhost:5003"
    ]
  }
}
